'''
# Extended CP 5
# get the first input (float)
side = float(input("Enter the cube's side length: "))
# while the user's input is not 0
while side != 0:    
    # claculate the volume of a cube
    vol = side * side * side
    
    # if the vol is less than 1000 cubed meters
    if vol < 1000:        
        # display the volume of the cube
        print(f'The volume of this cube is {vol}')
    # otherwise 
    else: 
        # display too large
        print('This cube is too large!')
    ## update user's input
    side = float(input("Enter the cube's side length: "))
#program ends
''' 
'''
# Extended CP 5 (variation 2)
# get the first input (float)
side = 10
# while the user's input is not 0
while side != 0:    
    side = float(input("Enter the cube's side length: "))
    if side == 0:
        break # - immediately ends the current while loop
        #continue #- returns control to the while loop condition
    # claculate the volume of a cube
    vol = side * side * side
    
    # if the vol is less than 1000 cubed meters
    if vol < 1000:        
        # display the volume of the cube
        print(f'The volume of this cube is {vol}')
    # otherwise 
    else: 
        # display too large
        print('This cube is too large!')

#program ends

'''


'''
# Extended CP 5 (variation 3)
# get the first input (float)
side = 10
# while the user's input is not 0
while side != 0:    
    side = float(input("Enter the cube's side length: "))
    if side == 0:
        break # - immediately ends the current while loop
    if side < 0:   
        continue # - returns control to the while loop condition
    # claculate the volume of a cube
    vol = side * side * side
    
    # if the vol is less than 1000 cubed meters
    if vol < 1000:        
        # display the volume of the cube
        print(f'The volume of this cube is {vol}')
    # otherwise 
    else: 
        # display too large
        print('This cube is too large!')

#program ends
'''

'''
# Extended CP 5 (variation 4)
# get the first input (float)
side = float(input("Enter the cube's side length: "))
while side < 0:
    side = float(input("Enter the cube's side length (non negative value): "))


# while the user's input is not 0
while side != 0:    
    # claculate the volume of a cube
    vol = side * side * side
    
    # if the vol is less than 1000 cubed meters
    if vol < 1000:        
        # display the volume of the cube
        print(f'The volume of this cube is {vol}')
    # otherwise 
    else: 
        # display too large
        print('This cube is too large!')
    ## update user's input
    side = float(input("Enter the cube's side length: "))
    while side < 0:
        side = float(input("Enter the cube's side length (non negative value): "))
#program ends
'''

