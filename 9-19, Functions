'''
def Rect_Area(side1, side2):
    return side1 * side2
    
def Rect_Perim(side1, side2):
    return (2 * side1 + 2 * side2)

if __name__ == "__main__":
    side1 = 5
    side2 = 10
    x = Rect_Area(side1, side2)
    # should return 50
    side3 = 3
    y = Rect_Perim(side1, side3)
    #should return 16
    print(x, y)
'''

'''
def CP7():
    #initialize some variables
    total = 0
    count = 0
    # Get the 1st integer user
    num = int(input())
    # while the entered number is not -999
    while num != -999:
        #accumulate the sum of all numbers
        total += num # total = total + num
        #keep count of all numbers
        count += 1 # count = count + 1
        #update the condition - get a new integer
        num = int(input())
    # check how numbers were entered
    if count == 0:
        print('The average of 0 numbers is 0')
    else:
        print(f'The average of {count} numbers is {total/count: .2f}')


#CP8
cont = 'yes'
while cont == 'yes':
    CP7()
    cont = input()
'''
























